{
    "extends": ["eslint: recomended", "plugin:import/errors", "prettier"],
    "rules": {
        "no-console": 1
    },
    "plugins": ["import"],
    "parserOptions": {
        "ecmasVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
